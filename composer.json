{
    "name": "afonsoinocente/simple-php-crawler",
    "description": "Creation of a simple and low level php crawler, using natives functions from php as curl, domdocument, domxpath, etc...",
    "authors": [
        {
            "name": "Afonso Inocente",
            "email": "afonsohenriqueinocente@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        },
        "files": [
            "src/helpers/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "require": {
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "start": "php -S 0.0.0.0:80 -t public",
        "test": "phpunit"
    }
}
